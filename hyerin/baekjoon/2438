import java.util.Scanner;

public class Main{
    public static void main(String[] args){
        Scanner sc = new Scanner(System.in);
        int cnt = sc.nextInt();
        
        for(int x=0; x < cnt; x++){
            for(int y=0; y< x+1; y++){
                System.out.print("*");
            }
             System.out.println("");
        }
    }
}
